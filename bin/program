#!/usr/bin/env bash
echo hello world
export
echo in dir `pwd`
for each in $(find . -type d | grep -vE "/.git$" | grep -vE "/.git/" | grep -vE "/.heroku$" | grep -vE "/.heroku/")
do
  echo DIR "$each"
  ls -la "$each"
done
echo DIR "$TEMP_APP_DIR"
ls -la "$TEMP_APP_DIR"

if [ ! -f "$TEMP_APP_DIR/testfile" ]
then
  lineCount=0
else
  lineCount=$(cat "$TEMP_APP_DIR/testfile")
fi
echo $((1+$lineCount)) > "$TEMP_APP_DIR/testfile"
cat "$TEMP_APP_DIR/testfile"

